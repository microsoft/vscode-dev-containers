{
	"definitionVersion": "0.200.8",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"rust:${VERSION}-1"
		]
	},
	"dependencies": {
		"image": "rust:1",
		"imageLink": "https://hub.docker.com/_/rust",
		"apt": [
			"lldb", 
			"python3-minimal",
			"gcc", 
			"libc6-dev"
		],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh"
		},
		"cargo": {
			"rls": null,
			"rustfmt": null,
			"rust-analysis": "rustc --version",
			"rust-src": "rustc --version",
			"clippy": "rustc --version"
		},
		"languages": {
			"Rust": {
				"cgIgnore": true
			}
		}
	}
}
