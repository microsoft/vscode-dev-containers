{
	"variants": ["3.0", "2.7", "2.6"],
	"definitionVersion": "0.201.9",
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"ruby:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"3.0": [ "ruby:${VERSION}-3" ],
			"2.7": [ "ruby:${VERSION}-2" ]
		}
	},
	"dependencies": {
		"image": "ruby:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/ruby",
		"debian": [{
				"cgIgnore": false,
				"name": "yarn"
			},
			"build-essential"
		],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm",
			"rbenv": "/usr/local/share/rbenv",
			"ruby-build": "/usr/local/share/ruby-build"
		},
		"gem": [
			"rake",
			"ruby-debug-ide",
			"debase"
		],
		"other":{
			"rvm": null
		},
		"languages": {
			"Ruby": {
				"cgIgnore": true,
				"path": "/usr/local"
			}
		}
	}
}
